.g-panel-group.g-form-setup-container
  position static
  width 100%
  margin 20px 0 0

  .g-panel-title-container
    padding 6px 10px !important

  .g-panel-content
    padding 5px 6px

.g-form-description-container
  margin-top 10px
  padding-top 12px
  border-top 1px solid #e1e1e1

.g-form-actions
  float right

  button
    margin-left 10px
    padding 4px 8px

.g-validation-failed-message
  padding 10px 0

span.g-bold-part
  color #555
  font-weight bold
